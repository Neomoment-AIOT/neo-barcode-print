generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "windows", "debian-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model counter_data {
  id                  Int       @id @default(autoincrement())
  unique_key          String    @unique @db.VarChar(255)
  counter             Int
  iqama_id            String    @db.VarChar(100)
  prescription_number String    @db.VarChar(100)
  date                String    @db.VarChar(20)
  device_id           String?   @db.VarChar(100)
  timestamp           DateTime? @default(now()) @db.Timestamp(6)
  Served              Boolean   @default(false)
  phar_id             Int       @default(0)
}

model metadata {
  key        String    @id @db.VarChar(50)
  value      Json
  updated_at DateTime? @default(now()) @db.Timestamp(6)
}

model pharmacy_id_table {
  id      Int  @id @default(autoincrement())
  phar_id Int?
}
